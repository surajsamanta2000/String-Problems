package com.jsp.Stringprogram;

public class ReverseS {

	public static void main(String[] args) {
		
		String a=" ";String b=" ";
		String str="java is easy";
		str=str+" ";
		
		
		for(int i=0;i<str.length();i++) {
			char ch=str.charAt(i);
			if(ch!=' ') {
				a=a+ch;
			}
			else {
				b=a+" "+b;
			   a="";
		}

	}

	System.out.println(b);


	
	}


}
